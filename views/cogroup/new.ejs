<!--Form for adding a new company group-->

<div id="thisPage" class='container'>
    <div class="row">
        <form action="/cogroup/create" method="post">
            <h1>
                <input name="name"
                       maxlength="4"
                       size="4"
                       style="height: 44px !important"
                       placeholder="ID">
            </h1>
            <h1>
                <input class="btn btn-success btn-lg"
                       type="submit"
                       value="CREATE COMPANY GROUP ID">
            </h1>
            <input type="hidden" name="_csrf" value="<%= _csrf %>" />
        </form>
    </div>

</div>
